# Environment Variables Template
# Copy this file to .env and fill in your actual values
# DO NOT commit .env file to version control

# Test Private Key for local development and testing
# This is used by test scripts in test/scripts/ directory
# Format: 0x followed by 64 hex characters
TEST_PRIVATE_KEY=

# Polygon RPC URL for forking and testing
# Get your RPC URL from Infura, Alchemy, or other providers
POLYGON_RPC_URL=https://polygon-mainnet.infura.io/v3/YOUR_API_KEY

# Optional: Other environment variables you might need
# ALLOW_TEST_SCRIPTS=true
