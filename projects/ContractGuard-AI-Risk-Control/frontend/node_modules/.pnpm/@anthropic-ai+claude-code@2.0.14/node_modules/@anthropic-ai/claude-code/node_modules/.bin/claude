#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/czy/contract-guard-ai/contract-guard-frontend/node_modules/.pnpm/@anthropic-ai+claude-code@2.0.14/node_modules/@anthropic-ai/claude-code/node_modules:/home/czy/contract-guard-ai/contract-guard-frontend/node_modules/.pnpm/@anthropic-ai+claude-code@2.0.14/node_modules/@anthropic-ai/node_modules:/home/czy/contract-guard-ai/contract-guard-frontend/node_modules/.pnpm/@anthropic-ai+claude-code@2.0.14/node_modules:/home/czy/contract-guard-ai/contract-guard-frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/czy/contract-guard-ai/contract-guard-frontend/node_modules/.pnpm/@anthropic-ai+claude-code@2.0.14/node_modules/@anthropic-ai/claude-code/node_modules:/home/czy/contract-guard-ai/contract-guard-frontend/node_modules/.pnpm/@anthropic-ai+claude-code@2.0.14/node_modules/@anthropic-ai/node_modules:/home/czy/contract-guard-ai/contract-guard-frontend/node_modules/.pnpm/@anthropic-ai+claude-code@2.0.14/node_modules:/home/czy/contract-guard-ai/contract-guard-frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
